    <script>
        var _frompagevalue = 'Return Login';
        function getQuerystring(name) {
            name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
            var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                results = regex.exec(location.search);
            return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
        }

        var submitted = false;
        var pageName = '';

        function buttonHandler(aButton, appendid) {
            var UserName_ctrlID = $("[id*='UserName']");
            TrackEvents('login screen clicks', { 'click option': 'submit' }, null, null, false);

            submitted = true;
            $('.error-server').hide();

            var sUid;
            var sPwd;
            var emailRegex = /.*@.*\..*/;
            var UserName_ctrlID = $("[id*='UserName']");
            var Password_ctrlID = $("[id*='Password']");
            var $error_email = $('.error-email');
            var $error_password = $('.error-password');
            sUid = UserName_ctrlID.val();
            sPwd = Password_ctrlID.val();

            if (aButton == "NEXT") {
                if (sUid == "") {
                    $error_email.text("Please enter an email address.").show();
                    UserName_ctrlID.focus();
                    return false;
                }
                else if (sUid && !emailRegex.test(sUid)) {
                    $error_email.text("Please enter a valid email address.").show();
                    UserName_ctrlID.focus();
                    var prev_val = sUid;
                    UserName_ctrlID.val(sUid);
                    return false;
                }
                else if (sPwd == "") {
                    $error_password.text("Please enter a password").show();
                    Password_ctrlID.focus();
                    return false;
                }

                else if (sUid.length > 50) {
                    $error_email.text("Max username length exceeded. Please reenter.").show();
                    UserName_ctrlID.val('');
                    UserName_ctrlID.focus();
                    return false;
                }
                else if (sPwd.length > 16) {
                    $error_password.text("Max password length exceeded. Please reenter.").show();
                    Password_ctrlID.val('');
                    Password_ctrlID.focus();
                    return false;
                }
                else if (PortalID !="37" && typeof fpCounterVal !="undefined" &&  fpCounterVal >= 3)
                {
                    var captchaResponse = grecaptcha.getResponse();
                    if (captchaResponse.length == 0) {                       
                        captchaValid = false;//reCaptcha not verified
                    }
                    else
                        captchaValid = true;

                    if (captchaValid == false) {
                        $("#errMsgCaptcha").text("Please validate captcha to login");
                        return false;
                    }
                    else if (captchaValid == true) {
                        $("#errMsgCaptcha").text('');
                        return true;
                    }
                }
                else {
                    return true;
                }
            }
        }

        function callGoogle() {
            TrackEvents('login screen clicks', { 'click option': 'google log in' }, null, null, false, true);

            var bKeepLoggedIn = false;
            if (document.getElementById("ChkRemember").checked == true) {
                bKeepLoggedIn = true;
            }

            if (typeof createCookie != "undefined") {
                var target = decodeURIComponent(getQueryString("targ"));
                target != "null" ? createCookie("SocialLoginTarg", target, 1) : "";
            }

            window.open('//www.myperfectresume.com/membership/google-controller.aspx?new=&productid=' + '17' + '&switchsession=0', 'Googlepopup', 'width=900,height=500,left=100');
        }
        //Blank function is used by segment.io after page load
       // function AsyncSegTrack() { }

        $('#lnkForgotPasswd').click(function () {
            TrackEvents('login screen clicks', { 'click option': 'forgot password' }, null, null, false);
        });

        $('#lnkSignUp').click(function () {
            TrackEvents('login screen clicks', { 'click option': "don't have an account" }, null, null, false);
        });

        $('#ChkRemember').change(function () {
            TrackEvents('login screen clicks', { 'click option': "stay signed in" }, null, null, false);
        });

        function callFacebook() {
            var bKeepLoggedIn = false;
            if (document.getElementById("ChkRemember").checked == true) {
                bKeepLoggedIn = true;
            }

            if (typeof createCookie != "undefined") {
                var target = decodeURIComponent(getQueryString("targ"));
                target != "null" ? createCookie("SocialLoginTarg", target, 1) : "";
            }
            window.open('//www.myperfectresume.com/membership/fb-controller.aspx', 'Fbpopup', 'width=900,height=500,left=100');
        }
        function OpenFeedBackWindow() {
            var FeedBack = "";
            FeedBack = '//www.myperfectresume.com/includes/FeedBack.aspx?Prod=17';
            window.open(FeedBack + "&Target=" + escape(location.href), 'livecareer', 'width=550,height=610,scrollbars=1,toolbar=0,resizable=1,menubar=0');
            return false;
        }

        var FreshDeskActivate = 'True';  
        var PortalCode = 'MPR';
        var PortalID = '16';
        if (PortalID == 20) {
            PortalCode = 'MPCL';
        }

        $(document).ready(function () {
            $('#aspnetForm').attr('inspectlet-form-analytics', 'sign-in');//MPR-25
             
            if (fpCounterVal >= 4) {                   
                $("#errMsgLastAttempt").text("You have one more attempt before your account is locked, please use forgot password to retrieve your password.");
            }
            
            var $error_email = $('.error-email');
            var $error_password = $('.error-password');
            var emailRegex = /.*@.*\..{2,}/;
            var $UserName = $('[id$="UserName"]');
            var $signup_loading = $('.signup-loading-img');
            var $fake_overlay = $('#fake-overlay');

            if ($signup_loading.length) {
                $UserName.blur();
                $fake_overlay.removeClass('dn');
            }

            $("[id$='UserName']").on('keyup', function () {
                if (submitted) {
                    var val = $(this).val();
                    var len = val.length;

                    if (len > 1) {
                        if (emailRegex.test(val)) {
                            $error_email.hide();
                        }

                        else {
                            $error_email.text('Please enter a valid email address.').show();
                        }
                    }
                    else {
                        $error_email.text('Please enter an email address.').show();
                    }
                }
            });

            $("[id$='Password']").on('keyup', function () {
                if (submitted) {
                    var val_len = $(this).val().length;
                    if (val_len > 1 && val_len < 17) {
                        $error_password.hide();
                    }
                }
            });

            var $fc_chat_layout = $('#fc_chat_layout');
            $(document.body).addClass('fcChatBody'); //Class is added for freshdesk Flashing

            if (FreshDeskActivate.toUpperCase() == 'TRUE') {

                var find_fc_chat_layout = setInterval(function () {
                    $fc_chat_layout = $('#fc_chat_layout');

                    if ($fc_chat_layout.length) {
                        clearInterval(find_fc_chat_layout);
                        $('body').removeClass('fcChatBody');

                        var $liveperson_chat_ctnr = $('#lpButDivID-1378283549245'),
                            $liveperson_chat_button = $liveperson_chat_ctnr.find('.lpStaticButtonTR');

                        $liveperson_chat_button.find('img').unwrap();
                        $liveperson_chat_ctnr.addClass('cp oh').css({ 'width': '259px', 'height': '44px', 'margin-bottom': '4px' });
                        $fc_chat_layout.css({ 'margin-bottom': '-45px' });
                        var fc_chat_container_h = $('#fc_chat_container').height();
                        var $fc_chat_title = $fc_chat_layout.find('.fc_pre-form').children('p:eq(0)').text();
                        $liveperson_chat_button.find('img').attr('title', $fc_chat_title);

                        $('body').find("#fc_chat_layout").on('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', function (e) {
                            if (parseInt($fc_chat_layout.css('bottom')) == -fc_chat_container_h) {
                                $fc_chat_layout.animate({ 'margin-bottom': '-45px' }, 100);
                            }
                        });

                        $liveperson_chat_ctnr.on({
                            mouseenter: function () { $(this).css({ 'background-position': '0 -40px' }); },
                            mouseleave: function () { $(this).css({ 'background-position': '0 0' }); },
                            click: function () { $('#fc_chat_header').click(); }
                        });
                    }
                }, 500);
            }
        });
    </script>

